{
  "name": "vue-outline",
  "version": "3.0.2",
  "description": "A vue tool to generate a navigation for a web page",
  "keywords": [
    "navigation",
    "outline",
    "vue",
    "vuejs",
    "目录",
    "目录自动生成",
    "导航",
    "大纲"
  ],
  "author": "Lushenggang <lushg-tcxg@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lushenggang/vue-outline.git"
  },
  "license": "MIT",
  "main": "dist/vue-outline.common.js",
  "unpkg": "dist/vue-outline.min.js",
  "module": "dist/vue-outline.esm.js",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-outline ./src/index.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "git push && npm run build-bundle"
  },
  "dependencies": {
    "uuid": "^3.3.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off",
      "no-console": "off",
      "no-irregular-whitespace": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
